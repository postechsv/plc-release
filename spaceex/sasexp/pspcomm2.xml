    <param name="signal" type="real" local="false" d1="1" d2="1" dynamics="any" controlled="false" />
    <param name="rcv" type="label" local="false" />
    <location id="1" name="Start" x="208.0" y="258.0" width="176.0" height="144.0">
      <invariant>CycleTimer &lt;= CycleDuration</invariant>
      <flow>wl2' == 0 - pumpOpen &amp;
CycleTimer' == 1 &amp;
pumpOpen' == 0</flow>
    </location>
    <location id="2" name="CalculateNext" x="666.0" y="141.0" width="192.0" height="142.0">
      <invariant>CycleTimer &lt;= CycleDuration</invariant>
      <flow>wl2' == 0 - pumpOpen &amp;
CycleTimer' == 1 &amp;
pumpOpen' == 0</flow>
    </location>
    <location id="3" name="comm" x="562.0" y="414.0" width="168.0" height="160.0">
